<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <style>
    table {
        width: 100%;
        border-collapse: collapse;
    }
    table, th, td {
        border: 1px solid black;
    }
    table th{
      background-color: green;
      color: white;
      font-size: 20px;
      text-align: center;
    }
    table td{
      text-align: center;
    }
    th, td {
        padding: 10px;
    }
</style>
</head>
<body>
  
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Title</th>
          <th>Price</th>
          <th>Rating</th>
          <th>Description</th>
          <th>cratedAt</th>
        </tr>
      </thead>

      <tbody>
         <% data.forEach((item)=>{  %>

          <tr>
            <td><%= item._id%></td>
            <td><%= item.title%></td>
            <td><%= item.price%></td>
            <td><%= item.rating%></td>
            <td><%= item.description%></td>
            <td><%= item.cratedAt%></td>
          </tr>

        <% }) %>
        </tbody>
    </table>
</body>
</html>